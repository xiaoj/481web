{
  "survey": [
    {
      "type": "image_slider",
      "name": "slidy",
      "label": "heyhey",
      "_rowNum": 2
    },
    {
      "type": "select_one",
      "name": "menu",
      "label": "Main Menu",
      "appearance": "grid",
      "_rowNum": 3,
      "param": "menu"
    },
    {
      "type": "select_one",
      "name": "diagnostic_menu",
      "label": "Diagnostics",
      "condition": "selected(data('menu'), 'diagnostics')",
      "_rowNum": 4,
      "param": "diagnostic_menu"
    },
    {
      "type": "select_one",
      "name": "procedures_menu",
      "label": "Procedures",
      "condition": "selected(data('menu'), 'procedures')",
      "_rowNum": 5,
      "param": "procedures_menu"
    },
    {
      "type": "select_one",
      "name": "calculators_menu",
      "label": "Calculators",
      "condition": "selected(data('menu'), 'calculators')",
      "_rowNum": 6,
      "param": "calculators_menu"
    },
    {
      "type": "note",
      "label": "INTUBATION",
      "image": "img/intubation.jpeg",
      "_rowNum": 8
    },
    {
      "type": "note",
      "label": "LUMBAR",
      "image": "img/lumbar.gif",
      "_rowNum": 10
    },
    {
      "type": "goto",
      "condition": "not(selected(data('menu'), 'calculators')) && not(selected(data('calculators_menu'), 'weight_converter'))",
      "_rowNum": 12,
      "param": "weight_converter_end"
    },
    {
      "type": "label",
      "_rowNum": 13,
      "param": "weight_converter"
    },
    {
      "type": "screen",
      "_rowNum": 14,
      "prompts": [
        {
          "type": "decimal",
          "name": "weightPound",
          "label": "Enter weight in pounds:",
          "_rowNum": 15
        },
        {
          "type": "note",
          "label": "<p>Baby weight is: {{calculates.convertToKilo}} kilograms</p>",
          "condition": "data('weightPound')",
          "_rowNum": 16
        },
        {
          "type": "decimal",
          "name": "weightKilo",
          "label": "Enter weight in kilograms:",
          "_rowNum": 17
        },
        {
          "type": "note",
          "label": "<p>Baby weight is: {{calculates.convertToPound}} pounds</p>",
          "condition": "data('weightKilo')",
          "_rowNum": 18
        }
      ]
    },
    {
      "type": "label",
      "_rowNum": 20,
      "param": "weight_converter_end"
    }
  ],
  "calculates": [
    {
      "name": "convertToKilo",
      "calculation": "(data('weightPound')/2.20462).toFixed(2)",
      "_rowNum": 2
    },
    {
      "name": "convertToPound",
      "calculation": "(data('weightKilo')*2.20462).toFixed(2)",
      "_rowNum": 3
    },
    {
      "name": "weightLossKg",
      "calculation": "data('currentWeightKg') - data('birthWeightKg')",
      "_rowNum": 4
    },
    {
      "name": "weightLossKgPerc",
      "calculation": "(calculates.weightLossKg() * 100.0 / data('birthWeightKg')).toFixed(2)",
      "_rowNum": 5
    }
  ],
