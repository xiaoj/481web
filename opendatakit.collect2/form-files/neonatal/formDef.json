      "param": "procedures_end"
    },
    {
      "type": "select_one",
      "name": "procedures_menu",
      "label": "Procedures",
      "appearance": "grid",
      "_rowNum": 7,
      "param": "procedures_menu"
    },
    {
      "type": "screen",
      "condition": "selected(data('procedures_menu'), 'intubation')",
