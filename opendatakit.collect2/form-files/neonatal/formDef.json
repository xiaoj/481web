  "calculates": [
    {
      "name": "convertToKilo",
      "calculation": "(data('weightPound')/2.20462).toFixed(2)",
      "_rowNum": 2
    },
    {
      "name": "convertToPound",
      "calculation": "(data('weightKilo')*2.20462).toFixed(2)",
      "_rowNum": 3
    },
    {
      "name": "weightLossKg",
      "calculation": "data('currentWeightKg') - data('birthWeightKg')",
      "_rowNum": 4
    },
    {
      "name": "weightLossKgPerc",
      "calculation": "(calculates.weightLossKg() * 100.0 / data('birthWeightKg')).toFixed(2)",
      "_rowNum": 5
    },
    {
      "name": "postureScore",
      "calculation": "(function() {\n  switch (data('posture_slider')) { \n    case('posture0'):\n      return 0;\n    case('posture1'):\n      return 1;\n    case('posture2'):\n      return 2;\n    case('posture3'):\n      return 3;\n   case('posture4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 6
    },
    {
      "name": "squareScore",
      "calculation": "(function() {\n  switch (data('square_slider')) { \n    case('square_window-1'):\n      return -1;\n    case('square_window0'):\n      return 0;\n    case('square_window1'):\n      return 1;\n    case('square_window2'):\n      return 2;\n    case('square_window3'):\n      return 3;\n   case('square_window4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 7
    },
    {
      "name": "armScore",
      "calculation": "(function() {\n  switch (data('arm_slider')) { \n    case('arm_recoil0'):\n      return 0;\n    case('arm_recoil1'):\n      return 1;\n    case('arm_recoil2'):\n      return 2;\n    case('arm_recoil3'):\n      return 3;\n   case('arm_recoil4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 8
    },
    {
      "name": "poplitealScore",
      "calculation": "(function() {\n  switch (data('popliteal_slider')) { \n    case('popliteal-1'):\n      return -1;\n    case('popliteal0'):\n      return 0;\n    case('popliteal1'):\n      return 1;\n    case('popliteal2'):\n      return 2;\n    case('popliteal3'):\n      return 3;\n   case('popliteal4'):\n      return 4;  \n   case('popliteal5'):\n      return 5;  \n  }\n}())",
      "_rowNum": 9
    },
    {
      "name": "scarfScore",
      "calculation": "(function() {\n  switch (data('scarf_slider')) { \n    case('scarf_sign-1'):\n      return -1;\n    case('scarf_sign0'):\n      return 0;\n    case('scarf_sign1'):\n      return 1;\n    case('scarf_sign2'):\n      return 2;\n    case('scarf_sign3'):\n      return 3;\n   case('scarf_sign4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 10
    },
    {
      "name": "heelScore",
      "calculation": "(function() {\n  switch (data('heel_slider')) { \n    case('heel_to_ear-1'):\n      return -1;\n    case('heel_to_ear0'):\n      return 0;\n    case('heel_to_ear1'):\n      return 1;\n    case('heel_to_ear2'):\n      return 2;\n    case('heel_to_ear3'):\n      return 3;\n   case('heel_to_ear4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 11
    },
    {
      "name": "totalNMScore",
      "calculation": "calculates.postureScore() + calculates.squareScore() + calculates.armScore() + calculates.poplitealScore() + calculates.scarfScore() + calculates.heelScore()",
      "_rowNum": 12
    },
    {
      "name": "skinScore",
      "calculation": "(function() {\n  switch (data('skin_menu')) { \n    case('skin-1'):\n      return -1;\n    case('skin0'):\n      return 0;\n    case('skin1'):\n      return 1;\n    case('skin2'):\n      return 2;\n    case('skin3'):\n      return 3;\n   case('skin4'):\n      return 4;  \n   case('skin5'):\n      return 5;  \n  }\n}())",
      "_rowNum": 13
    },
    {
      "name": "lanugoScore",
      "calculation": "(function() {\n  switch (data('lanugo_menu')) { \n    case('lanugo-1'):\n      return -1;\n    case('lanugo0'):\n      return 0;\n    case('lanugo1'):\n      return 1;\n    case('lanugo2'):\n      return 2;\n    case('lanugo3'):\n      return 3;\n   case('lanugo4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 14
    },
    {
      "name": "plantarScore",
      "calculation": "(function() {\n  switch (data('plantar_menu')) { \n    case('plantar-2'):\n      return -2;\n    case('plantar-1'):\n      return -1;\n    case('plantar0'):\n      return 0;\n    case('plantar1'):\n      return 1;\n    case('plantar2'):\n      return 2;\n    case('plantar3'):\n      return 3;\n   case('plantar4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 15
    },
    {
      "name": "breastScore",
      "calculation": "(function() {\n  switch (data('breast_menu')) { \n    case('breast-1'):\n      return -1;\n    case('breast0'):\n      return 0;\n    case('breast1'):\n      return 1;\n    case('breast2'):\n      return 2;\n    case('breast3'):\n      return 3;\n   case('breast4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 16
    },
    {
      "name": "eyeEarScore",
      "calculation": "(function() {\n  switch (data('eye_ear_menu')) { \n   case('eye_ear-2'):\n      return -2;  \n    case('eye_ear-1'):\n      return -1;\n    case('eye_ear0'):\n      return 0;\n    case('eye_ear1'):\n      return 1;\n    case('eye_ear2'):\n      return 2;\n    case('eye_ear3'):\n      return 3;\n   case('eye_ear4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 17
    },
    {
      "name": "genitalsScore",
      "calculation": "(function() {\n  switch (data('genitals_menu')) { \n    case('genitals-1'):\n      return -1;\n    case('genitals0'):\n      return 0;\n    case('genitals1'):\n      return 1;\n    case('genitals2'):\n      return 2;\n    case('genitals3'):\n      return 3;\n   case('genitals4'):\n      return 4;  \n  }\n}())",
      "_rowNum": 18
    },
    {
      "name": "totalPhysicalScore",
      "calculation": "calculates.skinScore() + calculates.lanugoScore() + calculates.plantarScore() + calculates.breastScore() + calculates.eyeEarScore() + calculates.genitalsScore()",
      "_rowNum": 19
    },
    {
      "name": "totalBallardScore",
      "calculation": "calculates.totalNMScore() + calculates.totalPhysicalScore()",
      "_rowNum": 20
    }
