      "prompts": [
        {
          "type": "note",
          "name": "intubation_link",
          "label": "<a href=\"http://www.youtube.com/watch?v=z7sog1n4Rgo\">Go to intubation video on Youtube</a>",
          "_rowNum": 9
        },
        {
          "type": "note",
          "name": "intubation_img",
          "image": "img/intubation.jpeg",
          "_rowNum": 10
        },
        {
          "type": "note",
          "label": "<b>INTUBATION VIDEO</b>",
          "_rowNum": 11
        },
        {
          "type": "note",
          "name": "intubation_vdo",
          "video": "video/video.mp4",
          "_rowNum": 12
        }
      ]
    },
    {
      "type": "note",
      "label": "LUMBAR",
      "condition": "selected(data('procedures_menu'), 'lumbar')",
      "image": "img/lumbar.gif",
      "_rowNum": 15
    },
    {
      "type": "select_one",
      "name": "ballardExam_menu",
      "label": "Ballard Exam",
      "condition": "selected(data('procedures_menu'), 'ballardExam')",
      "appearance": "grid",
      "_rowNum": 17,
      "param": "ballardExam_menu"
    },
    {
      "type": "note",
      "label": "New Exam !!",
      "condition": "selected(data('ballardExam_menu'), 'newExam')",
      "_rowNum": 18
    },
    {
      "type": "note",
      "label": "View Last Exam !!",
      "condition": "selected(data('ballardExam_menu'), 'saveExam')",
      "_rowNum": 19
    },
    {
      "type": "label",
      "_rowNum": 20,
      "param": "procedures_end"
    },
    {
      "type": "goto",
      "condition": "not(selected(data('menu'), 'calculators'))",
      "_rowNum": 21,
      "param": "calculators_end"
    },
    {
      "type": "select_one",
      "name": "calculators_menu",
      "label": "Calculators",
      "appearance": "grid",
      "_rowNum": 22,
      "param": "calculators_menu"
    },
    {
      "type": "screen",
      "_rowNum": 23,
      "prompts": [
        {
          "type": "decimal",
          "name": "weightPound",
          "label": "Enter weight in pounds:",
          "_rowNum": 24
        },
        {
          "type": "note",
          "label": "<p>Baby weight is: {{calculates.convertToKilo}} kilograms</p>",
          "condition": "data('weightPound')",
          "_rowNum": 25
        },
        {
          "type": "decimal",
          "name": "weightKilo",
          "label": "Enter weight in kilograms:",
          "_rowNum": 26
        },
        {
          "type": "note",
          "label": "<p>Baby weight is: {{calculates.convertToPound}} pounds</p>",
          "condition": "data('weightKilo')",
          "_rowNum": 27
        }
      ]
    },
    {
      "type": "label",
      "_rowNum": 29,
      "param": "calculators_end"
    },
    {
      "type": "note",
      "label": "THE END!",
      "_rowNum": 31
    }
  ],
  "calculates": [
    {
      "name": "convertToKilo",
      "calculation": "(data('weightPound')/2.20462).toFixed(2)",
      "_rowNum": 2
    },
    {
      "name": "convertToPound",
      "calculation": "(data('weightKilo')*2.20462).toFixed(2)",
      "_rowNum": 3
    },
    {
      "name": "weightLossKg",
      "calculation": "data('currentWeightKg') - data('birthWeightKg')",
      "_rowNum": 4
    },
    {
      "name": "weightLossKgPerc",
      "calculation": "(calculates.weightLossKg() * 100.0 / data('birthWeightKg')).toFixed(2)",
      "_rowNum": 5
    }
